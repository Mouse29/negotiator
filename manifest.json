{
	"manifest_version": 2,
	"version": "0.0.3.2",
	"name": "KISS Privacy",
	"minimum_chrome_version": "18.0.0.0",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"description": "Simple ad-blocking/privacy filter for advanced users.",
	"browser_action": {
		"default_icon": "images/icon16.png",
		"default_title": "Kiss Privacy",
		"default_popup": "popup.html"
	},
	"update_url": "http://silentorbit.com/extensions/updates",
	"homepage_url": "http://silentorbit.com/kiss/",
	"background": {
		"page": "background.html"
	},
	"options_page": "options.html",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"<all_urls>"
	],
	"incognito": "spanning"
}
